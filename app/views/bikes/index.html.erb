<html>
  <head>
  <link rel="stylesheet" href="style.css">
  </head>
  <body>

  <div id="container">
    <div id="navbar">
      <ul>
        <li><a href="bikes"> Bikes </a></li>
        <li><a href="/"> Home </a></li>
        <li><a href="bookings"> Bookings </a></li>
        <li><a href="profile"> Profile </a></li>
      </ul>
    </div>
    <div id="webtitle">
      <h1>Rent LeBikes</h1>
      <p>Travel far and cheap with your <strong>LeBike</strong> by your side!</p>
    </div>

    <div class="bikebanner">
      <div class="col-12 my-5 d-flex justify-content-center">
        <div id="bikecards">
          <div class="card">
            <img src="data:image/jpeg;base64,">

            <p><strong>Price:</strong>
            <p><strong>Location:</strong>
            <a href="http://127.0.0.1:3000/bikes" class="my-button">More Info</a>
          </div>

          <div class="card">
            <img src="data:image/jpeg;base64,">

            <p><strong>Price:</strong>
            <p><strong>Location:</strong>
            <a href="http://127.0.0.1:3000/bikes" class="my-button">More Info</a>
          </div>

          <div class="card">
            <img src="data:image/jpeg;base64,">
            <p><strong>Price:</strong>
            <p><strong>Location:</strong>
            <a href="http://127.0.0.1:3000/bikes" class="my-button">More Info</a>
          </div>



        </div>
      </div>
    </div>


  </div>
  <% @bikes.each do |bike| %>
    <%= render "bike", bike: bike %>
    <%= link_to "show this bike", bike %>
  <% end %>
  <%= link_to "Add a new bike ;)", new_bike_path %>

  <%= link_to "log_in", new_user_session_path %>
 </body>
</html>
